{
  "name": "hsa-backend",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "add:admin": "babel-node init.js --presets es2015,stage-2",
    "dev": "babel-node server.js --presets es2015,stage-2",
    "build": "rimraf dist/* && babel ./ -d dist --presets es2015,stage-2 --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./npm-debug.log,./README.md --copy-files --require babel-polyfill",
    "postinstall": "npm run build",
    "start": "node dist/server.js",
    "watch": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test": "nyc mocha --exit --require babel-core/register",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker": "node server.js "
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.299.0",
    "@supercharge/collections": "^3.1.4",
    "@types/twilio": "^3.19.3",
    "apple-signin-auth": "^1.4.1",
    "aws-sdk": "^2.1348.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "^4.18.2",
    "express-validation": "^1.0.2",
    "faker": "^4.1.0",
    "firebase-admin": "^11.5.0",
    "fs": "^0.0.1-security",
    "googleapis": "^105.0.0",
    "helmet": "^3.12.1",
    "html-to-text": "^8.2.1",
    "https": "^1.0.0",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "juice": "^8.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "mongoose": "^5.6.11",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "multer": "^1.3.1",
    "multer-s3": "^2.7.0",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "readline-sync": "^1.4.9",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "shortid": "^2.2.13",
    "shortid-36": "^0.1.2",
    "socket.io": "^2.2.0",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^3.0.10",
    "twilio": "^4.10.0",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "lint": "^0.8.19",
    "mocha": "^10.0.0",
    "nock": "^10.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "12",
    "npm": ">=6.4.1"
  },
  "nyc": {
    "include": [
      "src/controller/reportCtrl.js",
      "src/controller/adminCtrl.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "dist"
    ],
    "require": [],
    "reporter": [
      "text",
      "text-summary",
      "lcov",
      "lcovonly"
    ],
    "instrument": true
  }
}
